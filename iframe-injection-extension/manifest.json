{
    "name": "Embed external site",
    "version": "1",
    "manifest_version": 3,
    "content_scripts": [{
        "js": ["contentscript.js"],
        "matches": ["*://*/*"],
        "all_frames": true
    }],
    "web_accessible_resources": [{
        "resources": ["popup.html"],
        "matches": ["<all_urls>"]
    }]
}
