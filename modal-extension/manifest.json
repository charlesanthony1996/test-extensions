{
    "manifest_version": 3,
    "name": "Simple Modal Extension",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentscript.js"],
            "run_at": "document_end"
        }
    ]
}
