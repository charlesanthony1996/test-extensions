{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["dist/content.js", ""]
      }
    ],
    "web_accessible_resources": [{
      "matches": ["<all_urls>"],
      "resources": ["test.js"]
    }]
}